    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "lineWidth": 1, "fillOpacity": 20 }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "id": 11,
      "title": "Memory Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
